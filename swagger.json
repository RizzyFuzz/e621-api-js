      "summary": "Menampilkan metadata secara random dari e621",
      "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "description": "Query Pencarian",
            "name": "tags",
            "in": "query",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
              "description": "Content Found",
              "schema": {
